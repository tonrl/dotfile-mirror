                        "keyColor": "38;5;55"
                },
                {
                        "type": "memory",
                        "key": "  󰓡 Memory",
                        "keyColor": "38;5;55"
                },
                {
                        "type": "disk",
                        "key": "   Disk ",
                        "keyColor": "38;2;157;124;216"
                },
                {
                        "type": "battery",
                        "key": "  󰄌 Battery",
                        "keyColor": "38;2;157;124;216"
                },
                {
                        "type": "uptime",
                        "key": "  󰅐 Uptime",
                        "keyColor": "38;5;36"
                },
                {
                        "type": "localip",
                        "key": "  󱦂 IP",
                        "keyColor": "38;5;36"
                },
                {
                        "type": "custom",
                        "format":"╰───────────────────────────────────────────╯",
                        "outputColor": "38;5;60"
                },
                {
                        "type": "colors",
                        "paddingLeft": 2,
                        "symbol": "circle"
                },
                "break",


        ]
}



{
        "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",

        "logo": {
                "type" : "kitty-direct",
                "source": "$HOME/.config/fastfetch/logo/cow.png",
                // "source": "arch1",
                "width": 45,
                "height": 20,
                "padding" : {
                        "left" : 10,
                        "right" : 10,
                }
        },

        "display": {
                "separator" : "   ",
        },

        "modules": [
                {
                        "type": "custom",
                        "format":"  󰌢  System Information",
                        "outputColor": "38;5;244"
                },
                {
                        "type": "custom",
                        "format":"╭───────────────────────────────────────────╮",
                        "outputColor": "38;5;60"
                },
                {
                        "type": "os",
                        "key": "  {icon} Distro", // Just get your distro's logo off nerdfonts.com
                        "keyColor": "38;5;197"
                },
                {
                        "type": "kernel",
                        "key": "   Kernel",
                        "keyColor": "38;5;197"
                },
                {
                        "type": "packages",
                        "key": "  󰏖 Packerges",
                        "keyColor": "38;5;113"
                },
                {
                        "type": "editor",
                        "key": "   Editor",
                        "keyColor": "38;5;113"
                },
                {
                        "type": "wm",
                        "key": "  {icon} WM",
                        "keyColor": "38;2;255;157;100"
                },
                {
                        "type": "terminal",
                        "key": "   Terminal",
                        "keyColor": "38;2;255;157;100"
                },
                {
                        "type": "custom",
                        "format":"╰───────────────────────────────────────────╯",
                        "outputColor": "38;5;60"
                },
                "break",
                {
                        "type": "custom",
                        "format":"  󱁤  Hardware Information",
                        "outputColor": "38;5;244"
                },
                {
                        "type": "custom",
                        "format":"╭───────────────────────────────────────────╮",
                        "outputColor": "38;5;60"
                },
                {
                        "type": "cpu",
                        "key": "   CPU",
                        "format": "AMD Ryzen 5 PRO 7540U ({4})",
